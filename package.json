{
  "name": "16-june",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "make-user-model": "npx sequelize-cli model:generate --name user --attributes name:string,email:string,password:string",
    "make-user_site-model": "npx sequelize-cli model:generate --name user_sites --attributes name:string,user_id:integer,dimension:integer,lattitude:integer,longitude:integer",
    "make-saplings-model": "npx sequelize-cli model:generate --name Saplings --attributes site_id:integer,sapling_count:integer",
    "migrate": "set ENVIRONMENT=local&& npx sequelize-cli db:migrate",
    "down": "set ENVIRONMENT=local&&npx sequelize-cli db:migrate:undo",
    "make-seed": "set ENVIRONMENT=local&&npx sequelize-cli seed:generate --name demo-user",
    "run-seed": "set ENVIRONMENT=local&&npx sequelize-cli db:seed:all",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node server.js",
    "start:dev": "set ENVIRONMENT=local&& nodemon server.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "nodemon": "^2.0.16",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "pg-pool": "^3.5.1",
    "sequelize": "^6.20.1",
    "swagger-jsdoc": "^6.0.0",
    "swagger-ui-express": "^4.4.0"
  },
  "devDependencies": {
    "dotenv": "^16.0.1",
    "sequelize-cli": "^6.4.1"
  }
}
